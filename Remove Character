
/*
Remove the character from the string :
eg:
amaraoaa
a
then output is: mro
*/


class firstproject {



	public static void main(String[] args)
	{
		String intp="eer";

		char[] ss=intp.toCharArray();
		System.out.println(ss);
		Char toRemove = 'r';
		for (int i = 0; i <intp.length(); i++) {

			if(intp.charAt(i)==toRemove){
				ss[i]=' ';
			}

		}
		String opt = new String(ss);
		opt = opt.replaceAll("\\s", "");
		System.out.println(opt);

	}
}
